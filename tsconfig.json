{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "downlevelIteration": true,
    "target": "esnext",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "lib": ["dom", "esnext", "es6"],
    "jsx": "preserve"
  }
}
